{
    "name": "webmasterninja/socialite",
    "description": "Laravel wrapper around OAuth 1 & OAuth 2 libraries.",
    "keywords": ["oauth", "laravel"],
    "license": "MIT",
    "homepage": "https://www.webmasterninjas.com",
    "support": {
        "issues": "https://github.com/webmasterninja/socialite/issues",
        "source": "https://github.com/webmasterninja/socialite"
    },
    "authors": [
        {
            "name": "Stan Quinn",
            "email": "stanquinn@webmasterninjas.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0",
        "league/oauth1-client": "~1.0"
    },
    "require-dev": {
        "illuminate/contracts": "~5.7.0|~5.8.0|^6.0|^7.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "facebook/graph-sdk":  "5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Webmasterninja\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmasterninja\\Socialite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Webmasterninja\\Socialite\\SocialiteServiceProvider"
            ],
            "aliases": {
                "Socialite": "Webmasterninja\\Socialite\\Facades\\Socialite"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
